# Postgres (warehouse-db service inside Docker network)
PG_HOST=warehouse-db
PG_PORT=5432
PG_DB=etl
PG_USER=etl_user
PG_PASSWORD=<SET_ME>

# Paths used by your DAG inside the container
DATA_DIR=/opt/airflow/data
ARCHIVE_DIR=/opt/airflow/data_archive

# Optional: during dev set to false to stop moving inputs to archive each run
# ARCHIVE_ON_SUCCESS=true
